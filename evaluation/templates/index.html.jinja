{% extends "base.html.jinja" %} {% block content %}
<div class="container">
  <div class="row">
    <div class="col text-center mx-3 mt-3">
      <h1 class="h4">Fine-tuning CodeLlama-7B on Synthetic Training Data for Fortran Code Generation using PEFT</h1>
      <p class="mb-0">
        <a href="https://github.com/sohamgovande">Soham Govande</a>,
        <a href="https://github.com/taeukkang">Taeuk Kang</a>,
        <a href="https://github.com/andrewcshi">Andrew Shi</a>
      </p>
      <p>Stanford University</p>
    </div>
  </div>
  <div class="row">
    <div class="col table-responsive">
      <table class="table table-striped table-sm">
        <col />
        <colgroup span="3"></colgroup>
        <colgroup span="3"></colgroup>
        <tr>
          <td colspan="2"></td>
          <th colspan="3" scope="colgroup">Our model</th>
          <th colspan="3" scope="colgroup">CodeLlama-7B</th>
        </tr>
        <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Compiled</th>
          <th>Executed</th>
          <th>Correct</th>
          <th>Compiled</th>
          <th>Executed</th>
          <th>Correct</th>
        </tr>
        <tbody class="table-group-divider">
          {% for id in finetuned_codes %}
          <tr>
            <td>{{ id }}</td>
            <td>
              <a href="{{url_for('result', problem_id=id)}}">
                {{ leetcode_data[id]["title"] }}
              </a>
            </td>
            <td class="font-emoji">
              {{ "✅" if finetuned_evals[id]["compiled"] else "❌" }}
            </td>
            <td class="font-emoji">
              {{ "✅" if finetuned_evals[id]["executed"] else "❌" }}
            </td>
            <td class="font-emoji">
              {{ '✅' if finetuned_evals[id]['exec_success'] == 'full' else '⚠️'
              if finetuned_evals[id]['exec_success'] == 'partial' else '❌' }}
            </td>
            <td class="font-emoji">
              {{ "✅" if baseline_evals[id]["compiled"] else "❌" }}
            </td>
            <td class="font-emoji">
              {{ "✅" if baseline_evals[id]["executed"] else "❌" }}
            </td>
            <td class="font-emoji">
              {{ '✅' if baseline_evals[id]['exec_success'] == 'full' else '⚠️'
              if baseline_evals[id]['exec_success'] == 'partial' else '❌' }}
            </td>
          </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  </div>
</div>

<style>
  .font-emoji {
    font-family: emoji;
  }
</style>

{% endblock %}
